                    <div class="ns_panel_sub_top ns_panel_flex">
                        <div class="ns_panel_flex_item_auto ns_panel_flex_middle">
                            <!-- span id="ns_button_hero_skill_manage_prev"></span>
                            <span class="text_page_paging">
                                <span class="content_current_page">0</span>&nbsp;/&nbsp;<span class="content_total_page">0</span>
                            </span>
                            <span id="ns_button_hero_skill_manage_next"></span -->
                            <span id="ns_button_hero_skill_manage_sort">-</span>
                        </div>
                        <div class="ns_panel_flex_right">
                            <span id="ns_button_hero_skill_medal_prize">{{ i18n.t('medal_prize') }}</span>
                        </div>
                    </div>

                    <div class="ns_panel_basic ns_panel_margin_yy">
                        <div class="hero_skill_manage_list_wrap ns_panel_grid_2"></div>
                    </div>
                </div>

            </div>
        </div>
        <div class="dialog_footer"></div>
    </div>
</div>

<div id="ns_dialog_hero_skill_detail">
    <span id="ns_button_hero_skill_detail_sub_close"></span>
    <div class="dialog_wrap">
        <div class="dialog_header">
            <div class="dialog_pop_header">
                <span id="ns_button_hero_skill_detail_close"></span>
                <span class="content_pop_title"></span></div>
        </div>
        <div class="dialog_content">
            <div class="content">

                <div class="ns_panel_basic">
                    <div class="ns_panel_flex ns_panel_margin_10">
                        <div class="ns_panel_flex_item_auto skill_icon_wrapper">
                            <div class="skill_image"></div>
                        </div>
                        <div class="ns_panel_trans">
                            <div class="skill_description"></div>
                        </div>
                    </div>

                    <div class="ns_panel_margin_xx ns_panel_most">
                        <table class="ns_table_common">
                            <thead>
                            <tr>
                                <th class="col1">{{ i18n.t('possession') }}</th>
                                <th class="col2">{{ i18n.t('level') }}</th>
                            </tr>
                            </thead>
                            <tbody>
                            <tr>
                                <td class="col1"><span class="skill_count"></span></td>
                                <td><span class="skill_level"></span></td>
                            </tr>
                            </tbody>
                        </table>
                    </div>

                    <div class="ns_panel_pop_button">
                        <span id="ns_button_hero_skill_detail_equip">{{ i18n.t('equip') }}</span>
                        <span id="ns_button_hero_skill_detail_un_equip">{{ i18n.t('disarm') }}</span>
                        <span id="ns_button_hero_skill_detail_delete_equip">{{ i18n.t('remove') }}</span>
                    </div>

                </div>

            </div>
        </div>
        <div class="dialog_footer"></div>
    </div>
</div>

