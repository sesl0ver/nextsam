<div id="ns_dialog_hero_skill_manage_list">
    <span id="ns_button_hero_skill_manage_list_sub_close"></span>
    <div class="dialog_wrap">
        <div class="dialog_header">
            <div class="dialog_title">
                <span class="content_title">{{ i18n.t('choose_hero') }}</span>
            </div>
            <div class="dialog_header_left"><span id="ns_button_hero_skill_manage_list_close"></span></div>
            <div class="dialog_header_right"><span id="ns_button_hero_skill_manage_list_close_all"></span></div>
        </div>
        <div class="dialog_content">
            <div class="content">

                <div class="ns_panel_basic">

                    <div class="ns_panel_tab ns_panel_grid_3">
                        <span id="ns_button_hero_skill_manage_list_tab_stat">{{ i18n.t('ability_value') }}</span>
                        <span id="ns_button_hero_skill_manage_list_tab_officer">{{ i18n.t('office_and_status') }}</span>
                        <span id="ns_button_hero_skill_manage_list_tab_army">{{ i18n.t('military_aptitude') }}</span>
                    </div>

                    <div class="ns_panel_sub_top ns_panel_flex">
                        <div class="ns_panel_flex_item_auto ns_panel_flex_middle content_header_wrapper text_gray">
                            <span id="ns_button_hero_skill_manage_list_prev">&nbsp;</span>
                            <span class="text_page_paging">
                            <span class="content_current_page">0</span>&nbsp;/&nbsp;<span class="content_total_page">0</span>
                        </span>
                            <span id="ns_button_hero_skill_manage_list_next">&nbsp;</span>
                            <span id="ns_button_hero_skill_manage_list_sort">-</span>&nbsp;
                        </div>
                        <span class="text_status ns_panel_flex_right">
                        <span>{{ i18n.t('chosen_hero') }}</span>&nbsp;<span class="content_selected_hero_count">0</span>&nbsp;/&nbsp;<span class="content_max_selected_hero_count">1</span>
                    </span>
                    </div>

                    <div class="ns_panel_most ns_panel_trans">

                        <table class="ns_table_common content_hero_manage_table">
                            <thead>
                            <tr>
                                <th>{{ i18n.t('hero') }}</th>
                                <th>{{ i18n.t('hero_grade') }}</th>

                                <th class="filter_stat">{{ i18n.t('stats_leadership') }}</th>
                                <th class="filter_stat">{{ i18n.t('stats_military_force') }}</th>
                                <th class="filter_stat">{{ i18n.t('stats_intellect') }}</th>
                                <th class="filter_stat">{{ i18n.t('stats_politics') }}</th>
                                <th class="filter_stat">{{ i18n.t('stats_charm') }}</th>

                                <th class="filter_officer">{{ i18n.t('officer') }}</th>
                                <!-- <th class="cont_filter_officer">충성</th> -->
                                <th class="filter_officer">{{ i18n.t('skill') }}</th>
                                <th class="filter_officer">{{ i18n.t('slot') }}</th>
                                <th class="filter_officer">현황</th>

                                <th class="filter_army">{{ i18n.t('infantry') }}</th>
                                <th class="filter_army">{{ i18n.t('spearmen') }}</th>
                                <th class="filter_army">{{ i18n.t('pikeman') }}</th>
                                <th class="filter_army">{{ i18n.t('archer') }}</th>
                                <th class="filter_army">{{ i18n.t('cavalry') }}</th>
                                <th class="filter_army">{{ i18n.t('siege') }}</th>

                                <th>{{ i18n.t('status') }}</th>
                            </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>

                    </div>

                </div>

            </div>
        </div>
        <div class="dialog_footer"></div>
    </div>
</div>