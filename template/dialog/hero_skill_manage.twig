                    </div>

                    <div class="ns_panel_tab ns_panel_grid_5">
                        <!-- TODO 깃발 처리 필요 css after -->
                        <span id="ns_button_hero_skill_manage_tab_battle">{{ i18n.t('battle') }}</span>
                        <!-- div class="report_unread_blue cont_hero_skill_manage_flag_battle">N</div -->
                        <span id="ns_button_hero_skill_manage_tab_perform">{{ i18n.t('performance') }}</span>
                        <span id="ns_button_hero_skill_manage_tab_assign">{{ i18n.t('assign') }}</span>
                        <span id="ns_button_hero_skill_manage_tab_develop">{{ i18n.t('grow') }}</span>
                        <span id="ns_button_hero_skill_manage_tab_special">{{ i18n.t('special') }}</span>
